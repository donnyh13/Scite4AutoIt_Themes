				],
				"settings": {
					"foreground": "#CF6A4C" // Scite Style 3
				}
			},
			{
				"name": "Functions - BuiltIn",
				"scope": [
					"support.function.autoit", // Logan
					"support.function.au3" // Genius -- Added
				],
				"settings": {
					"foreground": "#DAD085" // Scite Style 4
				}
			},
			{
				"name": "Keyword: True/False, Null/Default, And/Or/Not",
				"scope": [
					"constant.language.autoit", // Logan
					"keyword.operator.logical.autoit", // Logan
					"keyword.control.au3", // Genius
