	"tokenColors": [
			{
				"name": "White Space / Preprocessor directives",
				"scope": [
					"string.parameter.directives.autoit", // Logan
					"support.function.pragma.au3", // Genius
					"keyword.control.pragma.key.au3", // Genius
					"string.unquoted.pragma.value.au3" // Genius
				],
				"settings": {
					"foreground": "#F8F8F8" // Scite Style 0
				}
			},
			{
				"name": "Line Comments",
				"scope": [
					"comment.line.semicolon.autoit", // Logan
					"comment.line.au3" // Genius
				],
				"settings": {
					"foreground": "#5F5A60", // Scite Style 1
					"fontStyle": "italic"
				}
			},
			{
				"name": "Block Comments",
				"scope": [
					"comment.block.autoit", // Logan
					"comment.block.au3" // Genius
				],
				"settings": {
					"foreground": "#5F5A60", // Scite Style  2
					"fontStyle": "italic"
				}
			},
			{
				"name": "Numbers",
				"scope": [
					"constant.numeric.autoit", // Logan
					"constant.numeric.au3", // Genius
