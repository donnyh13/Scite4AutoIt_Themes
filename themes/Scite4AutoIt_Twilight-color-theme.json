					// Keyword: Func, Const, Enum
					"storage.type.autoit", // Logan
					"storage.type.function.au3", // Genius
					"storage.type.enum.au3", // Genius
					// Keyword: Variable Scope etc
					"storage.modifier.autoit", // Logan
					"storage.modifier.au3", // Genius
					// Keyword: Conditional Statements etc.
					"keyword.control.autoit", // Logan
					"keyword.control.au3" //Genius
				],
				"settings": {
					"foreground": "#F9EE98" // Scite Style 5
				}
			},
			{
				"name": "AutoIt Macros",
				"scope": [
					"constant.other.autoit", // Logan
					"variable.other.constant.au3" // Genius
				],
				"settings": {
					"foreground": "#DADADA" // Scite Style 6
				}
			},
			{
				"name": "Includes and Strings",
				"scope": [
					"string",
					"string.parameter.import.autoit", // Logan
					"string.quoted.double.autoit", // Logan
					"string.quoted.single.autoit", // Logan
					"punctuation.definition.string.begin.autoit", // Logan
					"punctuation.definition.string.end.autoit", // Logan
					"punctuation.definition.string.begin.import.autoit", // Logan
					"punctuation.definition.string.end.import.autoit", // Logan
					"constant.character.escape.autoit", // Logan
