	"tokenColors": [
        {
            "name": "White Space / Preprocessor directives",
            "scope": [
                "string.parameter.directives.autoit", // Logan
                "support.function.pragma.au3", // Genius
                "keyword.control.pragma.key.au3", // Genius
                "string.unquoted.pragma.value.au3" // Genius
            ],
            "settings": {
                "foreground": "#F8F8F2" // Scite Style 0
            }
        },
        {
            "name": "Line Comments",
            "scope": [
                "comment.line.semicolon.autoit", // Logan
                "comment.line.au3" // Genius
            ],
            "settings": {
                "foreground": "#755E71", // Scite Style 1
            }
        },
        {
            "name": "Block Comments",
            "scope": [
                "comment.block.autoit", // Logan
                "comment.block.au3" // Genius
            ],
            "settings": {
                "foreground": "#755E71", // Scite Style  2
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric.autoit", // Logan
                "constant.numeric.au3", // Genius
                "constant.numeric.scientific_notation.au3", // Genius
                "constant.numeric.hex.au3", // Genius
                "constant.numeric.double.au3", // Genius
                "constant.numeric.integer.au3" // Genius
            ],
            "settings": {
                "foreground": "#AE81FF" // Scite Style 3
            }
        },
        {
            "name": "Functions - BuiltIn",
            "scope": [
				"support.function.autoit", // Logan
				"support.function.au3" // Genius -- Added
			],
            "settings": {
                "foreground": "#DAD085" // Scite Style 4
            }
        },
        {
            "name": "Keyword: True/False, Null/Default, And/Or/Not",
            "scope": [
                "constant.language.autoit", // Logan
                "keyword.operator.logical.autoit", // Logan
                "keyword.control.au3", // Genius
				"keyword.operator.logical.au3", // Genius -- Added
                // Keyword: Func, Const, Enum
                "storage.type.autoit", // Logan
                "storage.type.function.au3", // Genius
                "storage.type.enum.au3", // Genius
                // Keyword: Variable Scope etc
                "storage.modifier.autoit", // Logan
                "storage.modifier.au3", // Genius
                // Keyword: Conditional Statements etc.
                "keyword.control.autoit", // Logan
                "keyword.control.au3" //Genius
            ],
            "settings": {
                "foreground": "#66D9EF" // Scite Style 5
            }
        },
        {
            "name": "AutoIt Macros",
            "scope": [
                "constant.other.autoit", // Logan
                "variable.other.constant.au3" // Genius
            ],
            "settings": {
                "foreground": "#8996A8" // Scite Style 6
            }
        },
        {
            "name": "Includes and Strings",
            "scope": [
                "string.parameter.import.autoit", // Logan
                "string.quoted.double.autoit", // Logan
                "string.quoted.single.autoit", // Logan
                "punctuation.definition.string.begin.autoit", // Logan
                "punctuation.definition.string.end.autoit", // Logan
                "punctuation.definition.string.begin.import.autoit", // Logan
                "punctuation.definition.string.end.import.autoit", // Logan
                "constant.character.escape.autoit", // Logan
                "string.quoted.double.au3", // Genius
                "string.quoted.pragma.value.au3", // Genius
                "string.other.include.au3", // Genius
                "constant.character.escape.quoted.double.au3" // Genius
            ],
            "settings": {
                "foreground": "#E6DB74" // Scite Style 7
            }
        },
        {
            "name": "Operator: Brackets, Punctuation, Assignment, Math",
            "scope": [
                "keyword.operator.assignment.autoit", // Logan
                "keyword.operator.arithmetic.autoit", // Logan
                "keyword.operator.comparison.autoit", // Logan
                "punctuation.bracket.autoit", // Logan
                "punctuation.commadot.autoit", // Logan
                "keyword.control.underscore.autoit", // Logan
                "keyword.operator.au3", // Genius
                "keyword.control.underscore.au3" // Genius
            ],
            "settings": {
                "foreground": "#F92672" // Scite Style 8
            }
        },
        {
            "name": "User Variables",
            "scope": [
                "variable.other.autoit", // Logan
                "punctuation.definition.variable.autoit", // Logan
                "variable.other.au3" // Genius
            ],
            "settings": {
                "foreground": "#F8F8F2" // Scite Style 9
            }
        },
        {
            "name": "Send Keys",
            "scope": [
                "constant.other.send-key.autoit" // Logan
            ],
            "settings": {
                "foreground": "#8996A8" // Scite Style 10
            }
        },
        {
            "name": "Pre-Processors",
            "scope": [
                "keyword.control.import.autoit", // Logan
                "keyword.control.directives.autoit", // Logan
                "keyword.control.pragma.au3", // Genius
                "keyword.control.notrayicon.au3", // Genius
                "keyword.control.requireadmin.au3", // Genius
                "keyword.control.includeonce.au3", // Genius
                "keyword.control.onautoitstartregister.au3", // Genius
                "keyword.control.include.au3" // Genius
             ],
            "settings": {
                "foreground": "#8996A8" // Scite Style 11
            }
        },
        {
            "name": "Special -- AU3 Wrapper/Stripper etc",
            "scope": [
                "meta.preprocessor.autoit", // Logan
                "meta.preprocessor.au3", // Genius
                "keyword.control.pragma.au3" // Genius
             ],
            "settings": {
                "foreground": "#DADADA" // Scite Style 12
            }
        },
        {
            "name": "COM Objects",
            "scope": [ 
                "support.class.com.autoit", // Logan
                "support.type.property-name.au3" // Genius
        ],
            "settings": {
                "foreground": "#FD971F" // Scite Style 14
            }
        },
        {
            "name": "Functions - Standard UDF's",
            "scope": "support.function.other.autoit", // Logan
            "settings": {
                "foreground": "#FD971F" // Scite Style 15
            }
        },
        {
            "name": "Functions - File/User UDF's",
            "scope": [
                "entity.name.function.autoit", // Logan
                "entity.name.function.au3" // Genius
            ],
            "settings": {
                "foreground": "#FD831F", //? Scite Style 17 -- Added for cleaner look.
                "fontStyle": "underline"
            }
        },
